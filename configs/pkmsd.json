{
    "server": {
        "port": 2002,
        "corsPolicy": "permissive"
    },
    "repo": {
        "root_dir": "./public",
        "customUrlMappings": [
            {
                "url": "/$root",
                "target": "./public"
            },
            {
                "url": "/assets/dirview",
                "target": "./www/dirview"
            }
        ]
    }
}